load_email = (html, info, email) ->
  frame = $('#template-html')[0]
  pdf = $('#pdf_box').first()
  frame.contentWindow.document.body.innerHTML = html
  pdf.attr('data', '').hide()
  if email != ""
    pdf = $('#pdf_box').first().attr('data', email).show()

  $('#email_info').html(info)

  resizeFrame = () ->
    el = $('iframe').contents()[0]
    new_height = $(el).find('#templateContainer > tbody').outerHeight()
    frame.style.height = (new_height + 200) + 'px'
  resizeFrame()

